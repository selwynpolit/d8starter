{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "php": ">=5.6",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/address": "^1.6",
        "drupal/admin_toolbar": "^2.0",
        "drupal/allowed_formats": "^1.1",
        "drupal/anchor_link": "^1.6",
        "drupal/apply_for_role": "^1.0@alpha",
        "drupal/audiofield": "^1.9",
        "drupal/backup_migrate": "^4.0",
        "drupal/bamboo_twig": "^4.1",
        "drupal/bibcite": "^1.0@alpha",
        "drupal/blogapi": "^1.0@beta",
        "drupal/bootstrap": "^3.19",
        "drupal/bootstrap_layouts": "^5.1",
        "drupal/captcha": "^1.0@beta",
        "drupal/checklistapi": "^1.10",
        "drupal/ckeditor_media_embed": "^1.7",
        "drupal/ckeditor_uploadimage": "^1.7",
        "drupal/ckeditor_youtube": "^1.1",
        "drupal/colorbox": "^1.4",
        "drupal/colorbox_inline": "^1.1",
        "drupal/colorbox_load": "^1.0@RC",
        "drupal/console": "^1.0.2",
        "drupal/content_access": "^1.0@alpha",
        "drupal/convert_bundles": "^1.0@alpha",
        "drupal/core": "^8.7.0",
        "drupal/ctools": "^3.2",
        "drupal/date_recur": "^2.0",
        "drupal/datetime_extras": "^1.0@alpha",
        "drupal/devel": "^2.1",
        "drupal/duration_field": "^1.6",
        "drupal/dynamic_entity_reference": "^1.7",
        "drupal/entity_reference_display": "^1.2",
        "drupal/extlink": "^1.1",
        "drupal/feeds": "^3.0@alpha",
        "drupal/feeds_ical": "^1.1",
        "drupal/fences": "^2.0@alpha",
        "drupal/file_browser": "^1.1",
        "drupal/file_download": "^1.3",
        "drupal/file_entity": "^2.0@beta",
        "drupal/filefield_paths": "^1.0@beta",
        "drupal/fivestar": "1.x-dev",
        "drupal/flag": "^4.0@alpha",
        "drupal/focal_point": "^1.2",
        "drupal/fullcalendar_view": "^2.4",
        "drupal/geofield": "^1.3",
        "drupal/geofield_map": "^2.36",
        "drupal/group": "^1.0@RC",
        "drupal/honeypot": "^1.29",
        "drupal/imce": "^1.7",
        "drupal/inline_responsive_images": "^2.1",
        "drupal/invite": "^1.0@beta",
        "drupal/libraries": "^3.0@alpha",
        "drupal/linkchecker": "1.x-dev",
        "drupal/linkicon": "^1.4",
        "drupal/maillog": "1.x-dev",
        "drupal/menu_block": "^1.5",
        "drupal/menu_trail_by_path": "^1.1",
        "drupal/message": "^1.0@RC",
        "drupal/message_notify": "^1.0@beta",
        "drupal/message_subscribe": "^1.0@beta",
        "drupal/metatag": "^1.10",
        "drupal/module_filter": "^3.1",
        "drupal/oauth": "^2.1",
        "drupal/paragraphs": "^1.9",
        "drupal/pathauto": "^1.5",
        "drupal/permissions_by_term": "^2.10",
        "drupal/rabbit_hole": "^1.0@beta",
        "drupal/rename_admin_paths": "^1.1",
        "drupal/rules": "^3.0@alpha",
        "drupal/save_edit": "^1.2",
        "drupal/schema_metatag": "^1.3",
        "drupal/seo_checklist": "^4.1",
        "drupal/services": "^4.0@beta",
        "drupal/share_everywhere": "^1.1",
        "drupal/simple_sitemap": "^3.4",
        "drupal/simplenews": "^1.0@beta",
        "drupal/smart_trim": "^1.1",
        "drupal/social_media_links": "^2.6",
        "drupal/svg_image": "^1.9",
        "drupal/taxonomy_menu": "^3.4",
        "drupal/token": "^1.5",
        "drupal/twig_tweak": "^2.4",
        "drupal/twitter": "1.x-dev",
        "drupal/twitter_feed": "^1.3",
        "drupal/video": "^1.4",
        "drupal/video_embed_field": "^2.2",
        "drupal/views_bulk_operations": "^2.5",
        "drupal/views_entity_form_field": "^1.0@beta",
        "drupal/views_infinite_scroll": "^1.5",
        "drupal/views_slideshow": "^4.6",
        "drupal/views_slideshow_xtra": "^4.0@alpha",
        "drupal/votingapi": "^3.0@beta",
        "drupal/webform": "^5.4",
        "drupal/webform_views": "^5.0@alpha",
        "drupal/xmlrpc": "^1.0@beta",
        "drupal/xmlsitemap": "^1.0@alpha",
        "drush/drush": "^9.0.0",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "wikimedia/composer-merge-plugin": "^1.4",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "require-dev": {
        "webflo/drupal-core-require-dev": "^8.7"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        }
    }
}
